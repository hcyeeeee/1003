// 獲取當前毫秒數\n    // 設置結束時間 : 5月13號 15點0分0秒\n    time.setMonth(10); //   獲取當前 月份 (從 '0' 開始算)\n    time.setDate(26); //   獲取當前 日\n    time.setHours(0); //   獲取當前 時\n    time.setMinutes(0); //   獲取當前 分\n    time.setSeconds(0);\n    var endTime = time.getTime();\n\n    // 倒數計時: 差值\n    var offsetTime = (endTime - nowTime) / 1000; // ** 以秒為單位\n    var hr = parseInt(23 - hour); // 時\n    var day = parseInt(offsetTime / 60 / 60 / 24); // 時\n\n    aI[0].textContent = day + \"天\";\n    aI[1].textContent = hr + \"時\";\n\n}, 1000);\nexport default {\n    methods: {\n        function() {\n            console.log(nowTime);\n        },\n    }\n}\n</script>\n<style scoped>\n.addition {\n    position: relative;\n    top: -140px;\n\n}\n\n@media screen and (max-width: 500px) {}\n\np {\n    background-color: white;\n    border-radius: 30px 30px 0px 0px;\n    width: fit-content;\n    height: fit-content;\n    opacity: .9;\n    padding: 1.4rem 3rem;\n    color: black;\n}\n\n.box {\n    position: relative;\n    width: 100px;\n    margin-bottom: -2.4rem;\n    right: -170px;\n    top: 20px;\n    z-index: 1;\n}\n</style>\n"]}]}