當前 日\n    time.setHours(0); //   獲取當前 時\n    time.setMinutes(0); //   獲取當前 分\n    time.setSeconds(0);\n    var endTime = time.getTime();\n\n    // 倒數計時: 差值\n    var offsetTime = (endTime - nowTime) / 1000; // ** 以秒為單位\n\n    var day = parseInt(offsetTime / 60 / 60 / 24); // 時\n    aI[0].textContent = day + \"天\";\n\n\n}, 1000);\nexport default {\n    methods: {\n        function() {\n            console.log(nowTime);\n        },\n    }\n}\n</script>\n\n<style scoped>\n.box {\n    display: none;\n}\n\n@media screen and (min-width: 768px) {\n    .fa-angle-up {\n        display: none;\n    }\n\n    .box {\n        display: none;\n    }\n\n    .addition {\n        display: flex;\n        justify-content: end;\n        position: relative;\n        top: -77px;\n        margin: 0rem 2rem -6rem;\n    }\n\n    p {\n        position: relative;\n    }\n}\n</style>\n"]}]}