s.scrollY = window.scrollY\n        },\n    },\n    created() {\n        window.addEventListener('scroll', this.handleScroll)\n        window.addEventListener('scroll', this.scroll)\n    },\n}\n</script>\n\n<style scoped>\n.layout_addition {\n    position: fixed;\n    right: 0;\n    top: 0;\n    width: 70px;\n    top: 5rem;\n\n}\n\n.layout_addition img {\n    padding: 0.5rem\n}\n\n.layout_addition2 {\n    position: fixed;\n    left: 0;\n    top: 0;\n    margin-top: 60px;\n    margin-left: 20px;\n    width: 80px;\n    z-index: 9999;\n    top: 5rem;\n}\n\n.mobile {\n    display: none;\n}\n\n@media screen and (max-width: 768px) {\n    .layout_addition {\n        position: fixed;\n        right: -5px !important;\n        width: 70px;\n        top: 5rem;\n\n    }\n\n    .layout_addition img {\n        padding: 0.2rem 0.8rem\n    }\n\n    .layout_addition2 {\n        display: none;\n    }\n\n    .mobile {\n        display: block;\n    }\n\n}\n\n.fixed {\n    display: none;\n}\n\n.fa-solid {\n    position: fixed;\n    right: 0.5rem;\n    bottom: 5rem;\n    color: rgb(172, 188, 162);\n    background-color: rgb(226, 246, 214);\n    cursor: pointer;\n    background-color: #ffffffa6;\n    box-shadow: rgba(99, 99, 99, 0.2) 0px 2px 8px 0px;\n\n    border-radius: 10px;\n    padding: 2px 5px;\n    z-index: 1000;\n}\n</style>\n"]}]}