A,IAhBA,kBAgBA,CAAA;AAhBA,GAvBA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,SAAA,MAAA;AACA,SAAA,OAAA;AACA;AA5CA,CAAA","sourcesContent":["<template>\n    <div>\n        <div class=\"header\">\n            <div class=\"logo\">\n\n                <a href=\"\"> <img src=\"../assets/logo.png\" alt=\"民視新聞網\"></a>\n\n            </div>\n            <div class=\"navbar\">\n                <a v-for=\"(item, index) of list\" :key=\"index\" :href=\"item.anchor\">\n                    {{ item.title }}\n                </a>\n            </div>\n        </div>\n    </div>\n\n\n</template>\n\n<script>\nexport default {\n    data() {\n        return {\n            list: [\n                {\n                    title: '首頁',\n                    anchor: '#voting',\n                },\n                {\n                    title: '最新新聞',\n                    anchor: '#profile',\n                },\n                {\n                    title: '影音專區',\n                    anchor: '#porcess',\n                },\n                {\n                    title: 'LIVE新聞',\n                    anchor: '#news',\n                },\n            ],\n        }\n    },\n    methods: {\n        scroll() {\n            window.addEventListener('scroll', () => {\n                this.moveBar('#374674')\n            })\n        },\n\n        moveBar(color = '#0984e3') {\n            let bar = document.querySelector('.progress')\n            let w = Math.ceil(\n                (Math.round(window.pageYOffset) / (document.body.scrollHeight - window.innerHeight)) * 100\n            )\n            bar.style.width = w + '%'\n            bar.style.background = color\n        },\n\n        menu() { },\n    },\n    mounted() {\n        this.scroll()\n        this.moveBar()\n    },\n}\n</script>\n\n<style scoped>\n/* 漢堡選單 */\n.nav {\n    width: 250px;\n    padding: 1rem;\n    position: fixed;\n    background-color: rgba(255, 255, 255);\n    z-index: 998;\n    top: 3.3rem;\n    right: 1rem;\n    border-radius: 10px;\n    display: none;\n    box-shadow: 1px 2px 3px black;\n}\n\n.fa-list {\n    color: #595757;\n    cursor: pointer;\n    position: fixed;\n    z-index: 999;\n    top: 1rem;\n    right: 1rem;\n    background-color: white;\n    border-radius: 10px;\n    padding: 5px;\n    box-shadow: 1px 2px 3px black;\n}\n\n.fa-list:hover .nav {\n    display: block;\n}\n\n\n.nav a:hover::before {\n    content: '\\f061';\n    padding-right: 1rem;\n}\n\n\n.logo {\n    max-width: 100%;\n    width: 300px;\n}\n\n.logo img {\n    width: 200px;\n}\n\n@media screen and (max-width: 768px) {\n    .logo {\n        width: 200px;\n    }\n\n    .logo img {\n        width: 180px;\n    }\n}\n\n@media screen and (max-width: 450px) {\n    .logo {\n        width: 200px;\n    }\n\n    .logo img {\n        width: 160px;\n    }\n}\n\n/* 進度條 */\n.progress {\n    position: fixed;\n    top: 1px;\n    width: 0.5%;\n    height: 10px;\n    z-index: 999;\n}\n\na {\n    font-size: 1.2rem;\n    margin: 1rem;\n    display: block;\n    color: #595757;\n    font-weight: bolder;\n    transition: 0.3s;\n\n}\n\n.header {\n    display: grid;\n    grid-template-columns: 1fr 1fr;\n}\n\n.navbar {\n    justify-content: end;\n}\n</style>\n"],"sourceRoot":"src/components"}]}